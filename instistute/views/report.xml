<odoo>
	<data>

<record id="example_email_template1" model="mail.template">
          <field name="name">Example e-mail template</field>
          <field name="email_from">${}</field>
          <field name="subject">Congratz</field>
          <field name="email">${object.email}</field>
          <field name="lang">${object.lang}</field>
          <field name="model_id" ref="model_student_register"/>
          <field name="auto_delete" eval="True"/>
          <field name="body_html">
                  <![CDATA[
              <p>Dear ,<br/><br/>
              Good job, you've just created your first e-mail template!<br/></p>
                    Regards,<br/>
          
            ]]>
          </field>
</record>


	</data>

</odoo>
